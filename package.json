    "fs-extra": "^9.0.1",
    "ganache-cli": "^6.9.1",
    "mocha": "^7.2.0",
    "solc": "^0.6.8",
    "truffle-hdwallet-provider": "^1.0.17",
    "web3": "^1.2.8"
  },
  "config": {
    "ganache": {
